<div class="row sign-up-form">
  <div class="col l12">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Name: <%= @student.name %></span>
        <p>Email: <%= @student.email %></p>
        <p>Course Enrollements: None</p>
      </div>
    </div>
    <%= link_to '<i class="material-icons left">edit</i> Edit your profile'.html_safe, edit_student_path(@student), class: 'waves-effect waves-light btn teal right' %>
  </div>
</div>
